#map { width:610px; height:458px; margin:0 8px 0 0; border:1px solid #c3c3c1 }
.mapheader span { display:inline-block; margin-top:6px }
.mapheader .alert { background:url(<%= InsteddRails.url %>/theme/images/icons/18/orange/alert.png) no-repeat left 50%; padding-left:20px; color:#ff6602 }

.cluster {
  border: none;
  border-width: 0px;
  width: 26px;
  height: 36px;
  position: absolute;
  background-image: url(/assets/cluster.png);
}

.cluster-click {
  width: 26px;
  height: 36px;
  position: absolute;
  cursor: pointer;
}

.cluster-shadow {
  border: none;
  border-width: 0px;
  width: 31px;
  height: 36px;
  position: absolute;
  background-image: url(/assets/cluster.png);
  background-repeat: no-repeat;
  background-position: right top;
}

.cluster.inactive {
  background-image: url(/assets/cluster_inactive.png);
}

.cluster.target {
  background-image: url(/assets/cluster_target.png);
}

.cluster-count {
  border: none;
  border-width: 0px;
  border: 1px solid black;
  border-radius: 4px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
  -css3-background: linear-gradient(90deg, #888888, black);
  background: -moz-linear-gradient(top, black 0%, #888888 100%);
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, black), color-stop(100%, #888888));
  padding: 0px 2px 0px 2px;
  background-color: black;
  color: white;
  display: table-cell;
  text-align: center;
  min-width: 15px;
  height: 13px;
  line-height: 13px;
  font-size: 11px;
  position: absolute;
}

.cluster-count-click {
  min-width: 15px;
  height: 13px;
  position: absolute;
  cursor: pointer;
}

.orightexpand {
  display:none;
}
